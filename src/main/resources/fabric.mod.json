{
  "schemaVersion": 1,
  "id": "worldmesher",
  "version": "${version}",
  "name": "Worldmesher",
  "description": "Library for meshing world sections into self-contained renderable objects",
  "authors": [
    "glisco"
  ],
  "contact": {
    "sources": "https://github.com/gliscowo/worldmesher",
    "issues": "https://github.com/gliscowo/worldmesher/issues"
  },
  "license": "MIT",
  "environment": "client",
  "icon": "assets/worldmesher/icon.png",
  "accessWidener": "worldmesher.accesswidener",
  "mixins": [
    "worldmesher.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    },
    "worldmesher:fabric_api_build_version": "${fabric_api_version}"
  },
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": ">=0.85.0",
    "minecraft": ">=1.20.3"
  }
}
